# JWT Secret Key
SECRET_KEY=dev-secret-key-change-in-prod

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/pytutor

# ==================== Qdrant Cloud ====================
# Lấy URL và API Key từ Qdrant Cloud Console
# https://cloud.qdrant.io/
QDRANT_URL=https://your-cluster.region.aws.cloud.qdrant.io:6333
QDRANT_API_KEY=your_qdrant_api_key_here

# ==================== Groq LLM ====================
# Lấy API Key từ https://console.groq.com/
GROQ_API_KEY=your_groq_api_key_here
GROQ_MODEL=llama-3.1-8b-instant

# ==================== Sandbox Configuration ====================
SANDBOX_IMAGE=python-sandbox

# ==================== Feature Flags ====================
ENABLE_WS_TERMINAL=true
CORS_ALLOW_ORIGINS=*
